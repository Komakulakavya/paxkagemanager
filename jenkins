pipeline{
 agent any
 stages{
  stage('setup'){
   steps{
    echo 'building dependencies'
    bat '''pip install -r requirements.txt'''
   }
  }
  stage('build'){
   steps{
    echo 'building app'
    bat '''python app.py'''
   }
  }
 }
 post {
 success{
  echo 'build success'
 }
 failure
{
 echo 'build failure'
}
 }
}
